#!/usr/bin/make -f

# See debhelper(7) (uncomment to enable).
# Output every command that modifies files on the build system.
#export DH_VERBOSE = 1


%:
	dh $@


override_dh_install:
	dh_install
	#mkdir debian/palemoon-extq/
	dh_modify_debdir "" debian/palemoon-extq/

override_dh_builddeb:
	dh_builddeb
	dh_build_qinstaller
